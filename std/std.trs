fn str-debug
    string : do
    str2countstr
    "\"" pr
    let ptr count do
        0
        count repeat
            ptr swap
            let a b do
                b a b
            end
            ptr+ memload
            
            dup "\n" == if
                "\\n" pr
            else dup "\r" == if
                "\\r" pr
            else
                dup pr
            end end
            
            drop
            1 +
        end
        drop
        ptr memfree
    end
    "\"\n" pr
end

fn str-remove-final
    string number : string do
    swap 1 memalloc
    swap str2countstr

    let remove newstrcount str count do
        0 newstrcount memsave
        ""
        count remove - repeat
            // get n-th char of str
            str newstrcount memload ptr+ memload
            
            str+

            // increment newstrcount
            newstrcount memload 1 + newstrcount memsave
        end
        
        newstrcount memfree
        str memfree
    end
end

